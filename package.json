{
  "name": "yalru",
  "version": "0.0.1",
  "description": "Yet another LRU(Least Recently Used) cache for JavaScript.",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "prebuild": "npm run lint",
