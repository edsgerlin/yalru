  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edsgerlin/yalru/issues"
  },
  "homepage": "https://github.com/edsgerlin/yalru#readme",
  "dependencies": {
    "@types/chai": "~4.1.1",
    "@types/mocha": "~2.2.46",
    "@types/node": "~8.5.9",
    "@types/sinon": "~4.1.3",
    "chai": "~4.1.2",
    "mocha": "~4.1.0",
    "sinon": "~4.1.6"
  },
  "devDependencies": {
    "tslint": "~5.9.1",
    "typescript": "~2.6.2"
  }
}
