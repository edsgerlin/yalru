    "lint": "tslint --project tsconfig.json --fix",
    "pretest": "npm run build",
    "test": "mocha dist/test/"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edsgerlin/yalru.git"
  },
